<odoo>
  <data>
    
       
      <record id="view_order_form_crml" model="ir.ui.view">
          <field name="name">view.sale.order.form.lista_contactos</field>
          <field name="model">sale.order</field>
          <field name="inherit_id" ref="sale.view_order_form"/>
          <field eval="16" name="priority"/>
          <field name="arch" type="xml">
              <data>
                  <xpath expr="//field[@name='partner_id']" position="after">
                      <field name="lista_contactos"/>
                  </xpath>
              </data>
          </field>
      </record>

      <record id="view_move_form_crml" model="ir.ui.view">
          <field name="name">view.account.move.form.lista_contactos</field>
          <field name="model">account.move</field>
          <field name="inherit_id" ref="account.view_move_form"/>
          <field eval="16" name="priority"/>
          <field name="arch" type="xml">
              <data>
                  <xpath expr="//field[@name='partner_id']" position="after">
                      <field name="lista_contactos"/>
                  </xpath>
              </data>
          </field>
      </record>
      
  </data>
</odoo>